{
	"format_version": "1.8.0",
	"animations": {
		"animation.coelacanth.swim": {
			"loop": true,
			"override_previous_animation": true,
			"bones": {
				"tail": {
					"rotation": [0, "-math.sin((v.current_anim_time - v.last_anim_time) * 179 - 20) * 12", 0]
				},
				"bone2": {
					"rotation": [0, "-math.cos((v.current_anim_time - v.last_anim_time) * 180) * -20.5", 0]
				},
				"head": {
					"rotation": [0, "-math.cos((v.last_anim_time - v.current_anim_time) * 180) * 7.5;", 0]
				},
				"lateral_fin1": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 170) * 5", 0]
				},
				"lateral_fin2": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 190) * 5", 0]
				},
				"body": {
					"rotation": [0, "-math.cos((v.current_anim_time - v.last_anim_time) * 180) * 7.5", 0]
				},
				"lateral_fin4": {
					"rotation": [0, "math.sin((q.anim_time + q.modified_distance_moved)* 169) * 5", 0]
				},
				"lateral_fin3": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 189) * 5", 0]
				}
			}
		},
		"animation.coelacanth.flop": {
			"loop": true,
			"bones": {
				"root": {
					"rotation": [0, "math.cos(variable.animationamountblend * 45.0) * 4.0", "variable.zrot"]
				},
				"body": {
					"rotation": [0, "math.cos(variable.animationamountblend * 45.0) * -5.5", 0]
				},
				"tail": {
					"rotation": [0, "math.cos(variable.animationamountblend * 45.0) * -12.0", 0]
				},
				"bone2": {
					"rotation": [0, "math.cos(variable.animationamountblend * 45.0) * -15.0", 0]
				}
			}
		},
		"animation.pitch_n_roll": {
      "loop": true,
      "bones": {
        "root": {
          "rotation": [
            "q.body_x_rotation",
            0,
            "variable.roll*-0.66"
          ],
          "position": [
            "variable.roll*0.01",
            0,
            "q.body_x_rotation*0.025"
          ]
        }
      }
    },
		"animation.coelacanth.test_bb": {
			"loop": true,
			"animation_length": 2,
			"override_previous_animation": true,
			"bones": {
				"tail": {
					"rotation": [0, "-math.sin((q.anim_time) * 179 - 20) * 12", 0]
				},
				"bone2": {
					"rotation": [0, "-math.cos((q.anim_time) * 180) * -20.5", 0]
				},
				"head": {
					"rotation": [0, "-math.cos((q.anim_time) * 180) * 5.5;", 0],
					"position": ["math.cos((q.anim_time) * 180) * 0.3;", 0, 0]
				},
				"lateral_fin1": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 170) * 5", 0]
				},
				"lateral_fin2": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 190) * 5", 0]
				},
				"body": {
					"rotation": [0, "-math.cos((q.anim_time) * 180) * 7.5", 0]
				},
				"lateral_fin4": {
					"rotation": [0, "math.sin((q.anim_time + q.modified_distance_moved)* 169) * 5", 0]
				},
				"lateral_fin3": {
					"rotation": [0, "math.cos((q.anim_time + q.modified_distance_moved)* 189) * 5", 0]
				}
			}
		}
	}
}