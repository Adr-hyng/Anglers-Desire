{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "yn:fish_caught_event_particle",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/AquaticReelz/fish_caught_2"
			}
		},
		"components": {
			"minecraft:emitter_local_space": {
				"position": true,
				"rotation": false
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": 1
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 1
			},
			"minecraft:emitter_shape_point": {
				"offset": [0, 0.3, 0]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 1,
				"expiration_expression": "!q.is_on_screen || q.property('yn:particle_state') == 0 ? 1 : 0"
			},
			"minecraft:particle_expire_if_in_blocks": ["minecraft:water", "minecraft:flowing_water"],
			"minecraft:particle_motion_parametric": {
				"relative_position": [0, "!q.is_on_screen || q.property('yn:particle_state') == 0 ? -2 : 1", 0]
			},
			"minecraft:particle_appearance_billboard": {
				"size": [1, 1],
				"facing_camera_mode": "rotate_xyz",
				"uv": {
					"texture_width": 16,
					"texture_height": 160,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [16, 16],
						"step_UV": [0, 16],
						"frames_per_second": 12,
						"max_frame": 6
					}
				}
			}
		}
	}
}