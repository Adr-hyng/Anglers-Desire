{
	"format_version": "1.10.0",
	"particle_effect": {
		"description": {
			"identifier": "yn:entity_water_splash_exit",
			"basic_render_parameters": {
				"material": "particles_alpha",
				"texture": "textures/particle/water_splash"
			}
		},
		"curves": {
			"variable.e_acc_y": {
				"type": "linear",
				"input": "v.particle_age",
				"horizontal_range": "variable.max_lifetime",
				"nodes": [50, -22.36, -30, -30, -30, -30, -30, -30, 0]
			},
			"variable.scale_droplet": {
				"type": "bezier",
				"input": "v.particle_age",
				"horizontal_range": "v.particle_lifetime",
				"nodes": [0.4, 0.32, 0, 0]
			}
		},
		"events": {
			"ripple_main": {
				"particle_effect": {
					"effect": "sp:ripple_main",
					"type": "emitter",
					"pre_effect_expression": "v.rand_size = 1.5;\nvariable.y_offset = 0;"
				}
			},
			"splash_foam": {
				"particle_effect": {
					"effect": "sp:splashfoam",
					"type": "emitter",
					"pre_effect_expression": "v.min_y_offset = 0.0;\nv.max_y_offset = 0.0;\nv.min_lifetime = 2;\nv.max_lifetime = 3;\nv.gravity = -12.81;\nv.max_count = 1;\nv.width = 1;"
				}
			},
			"on_particle_ends": {
				"expression": "variable.on_end = 1;"
			},
			"ripple": {
				"particle_effect": {
					"effect": "sp:ripple",
					"type": "emitter"
				}
			}
		},
		"components": {
			"minecraft:emitter_initialization": {
				"creation_expression": "variable.splash_spread = 5;variable.splash_radius = 3;variable.min_splashes = 30;variable.max_splashes = 50;variable.on_end = 0;variable.max_lifetime = 2;"
			},
			"minecraft:emitter_rate_instant": {
				"num_particles": "math.random(variable.min_splashes, variable.max_splashes)"
			},
			"minecraft:emitter_lifetime_once": {
				"active_time": 3
			},
			"minecraft:emitter_shape_point": {
				"offset": [0, 0.2, 0],
				"direction": ["math.random(-variable.splash_spread, variable.splash_spread)", 1, "math.random(-variable.splash_spread, variable.splash_spread)"]
			},
			"minecraft:particle_lifetime_expression": {
				"max_lifetime": 2,
				"expiration_expression": "variable.on_end == 1"
			},
			"minecraft:particle_expire_if_in_blocks": ["minecraft:water", "minecraft:flowing_water"],
			"minecraft:particle_lifetime_events": {
				"creation_event": "ripple_main",
				"expiration_event": ["ripple", "splash_foam"],
				"timeline": {
					"0.10": "splash_flash"
				}
			},
			"minecraft:particle_initial_spin": {
				"rotation": "math.random(-180, 180)"
			},
			"minecraft:particle_initial_speed": "math.random(-v.splash_radius, v.splash_radius)",
			"minecraft:particle_motion_dynamic": {
				"linear_acceleration": ["math.random(-variable.splash_radius, variable.splash_radius)", "variable.e_acc_y", "math.random(-variable.splash_radius, variable.splash_radius)"]
			},
			"minecraft:particle_appearance_billboard": {
				"size": ["variable.scale_droplet", "variable.scale_droplet"],
				"facing_camera_mode": "lookat_xyz",
				"uv": {
					"texture_width": 128,
					"texture_height": 8,
					"flipbook": {
						"base_UV": [0, 0],
						"size_UV": [8, 8],
						"step_UV": [16, 0],
						"frames_per_second": 4,
						"max_frame": 4
					}
				}
			},
			"minecraft:particle_motion_collision": {
				"coefficient_of_restitution": 0.5,
				"collision_radius": 0.01,
				"expire_on_contact": true
			},
			"minecraft:particle_appearance_tinting": {
				"color": [1, 1, 1, 1]
			}
		}
	}
}