{
  "format_version": "1.12.0",
  "minecraft:entity": {
    "description": {
      "identifier": "minecraft:fishing_hook",
      "is_spawnable": false,
      "is_summonable": false,
      "is_experimental": false,
      "properties": {
        "yn:current_biome_bit": {
          "type": "int",
          "client_sync": true,
          "range": [0, 16],
          "default": 0
        },
        "yn:current_weather_bit": {
          "type": "int",
          "client_sync": true,
          "range": [0, 2],
          "default": 0
        }
      }
    },
    
    "component_groups": {
      "loot_jungle": {
        "minecraft:loot": {
          "table": "loot_tables/gameplay/jungle_fishing.json"
        }
      }
    },

      "components": {
        "minecraft:collision_box": {
          "width": 0.15,
          "height": 0.15
        },
        "minecraft:projectile": {
          "on_hit": { "stick_in_ground": {} }
        },
        "minecraft:loot": {
          "table": "loot_tables/gameplay/fishing.json"
        },
        "minecraft:physics": {
        },
        "minecraft:pushable": {
          "is_pushable": false,
          "is_pushable_by_piston": true
        },
        "minecraft:conditional_bandwidth_optimization": {
          "default_values": {
            "max_optimized_distance": 80.0,
            "max_dropped_ticks": 7,
            "use_motion_prediction_hints": true
          }
        }
      },

      "events": {
        "minecraft:entity_spawned": {
          "sequence": [
            {
              "filters": {
                "test": "weather_at_position",
                "value": "clear"
              },
              "set_property": {
                "yn:current_weather_bit": 0
              }
            },
            {
              "filters": {
                "test": "weather_at_position",
                "value": "rain"
              },
              "set_property": {
                "yn:current_weather_bit": 1
              }
            },
            {
              "filters": {
                "test": "weather_at_position",
                "value": "thunderstorm"
              },
              "set_property": {
                "yn:current_weather_bit": 2
              }
            },
            {
              "filters": {
                "test": "is_biome",
                "value": "jungle"
              },
              "add": { "component_groups": [ "loot_jungle" ] },
              "set_property": {
                "yn:current_biome_bit": 1
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 2
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "deep_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 3
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "frozen_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 4
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "deep_frozen_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 5
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "cold_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 6
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "deep_cold_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 7
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "lukewarm_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 8
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "deep_lukewarm_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 9
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "warm_ocean"
              },
              "set_property": {
                "yn:current_biome_bit": 10
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "mangrove_swamp"
              },
              "set_property": {
                "yn:current_biome_bit": 11
              }
            },
            {
              "filters": {
                "test": "is_biome",
                "value": "swamp"
              },
              "set_property": {
                "yn:current_biome_bit": 11
              }
            },
            {
              "filters": {
                "test": "has_biome_tag",
                "value": "lush_caves"
              },
              "set_property": {
                "yn:current_biome_bit": 12
              }
            }
          ]
        }
      }
    }
  }